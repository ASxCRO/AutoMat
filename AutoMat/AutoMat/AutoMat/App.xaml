<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AutoMat.App">
    <Application.Resources>
        <ResourceDictionary>
            <Color x:Key="MyAccent">White</Color>
            <Color x:Key="MyWhite">#FFFFFF</Color>
            <Color x:Key="MyBlack">#000</Color>

            <Color x:Key="MyPageBackground">#E6E6E6</Color>
            <Color x:Key="MyCardBackground">#FFFFFF</Color>


            <Color x:Key="PrimaryColor">#0078D7</Color>
            <Color x:Key="SecondaryColor">white</Color>
            <Color x:Key="TertiaryColor">#00A4EF</Color>
            <Color x:Key="QuaternaryColor">#7FBA00</Color>
            <Color x:Key="QuinaryColor">#f1f1f1</Color>

            <Color x:Key="TextColor">#000</Color>
            <Color x:Key="SecondaryTextColor">#ddd</Color>
        </ResourceDictionary>
    </Application.Resources>
</Application>